									time(datetime="2018") April 13, 2018
							li
								article.post-light
									p.post-light__title: a(href=postLink) How We Measure the Increasing Vlogs' Ad Potential
									time(datetime="2018") March 25, 2018

					.cell-xs-4.cell-sm-6.cell-md-2.cell-lg-2.cell-xl-1.footer-corporate__column
						h4.heading-bordered Links
						ul.list-xxs.list_darker
							li: a(href='index.html') Home
							li: +equalLink('About us')
							li: +equalLink('Services')
							li: +equalLink('Our Work')
							li: +equalLink('News')
							li: +equalLink('Contacts')

		.footer-corporate__aside.bg-gray-4.text-center.novi-background.bg-cover
			.shell.shell-fluid.shell-condensed
				.range.range-20.range-fix.range_xl-ten.footer-corporate__range
					.cell-sm-8.cell-xl-6.footer-corporate__aside-column.text-sm-left
						include ../includes/_rights

					.cell-sm-4.cell-xl-4.footer-corporate__aside-column.text-sm-right
						ul.list-inline-xxs
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-twitter(href='#')
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-facebook(href='#')
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-instagram(href='#')
	="\n"

	// Page Footer
	include ../sections/_footer-corporate



