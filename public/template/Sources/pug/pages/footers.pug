extends  ../_skeleton

//- Includes
include ../elements/_ui-quote-light
include ../elements/_ui-quote-creative

block variables
	- var activeIndex = 7
	- var pageName = 'Footers'


block page
	// Page Header
	include ../sections/_header-fullwidth

	section.breadcrumbs-custom.parallax-container(data-parallax-img=src = 'images/bg-image-11.jpg')
		.parallax-content
			.shell
				h2.breadcrumbs-custom__title Footers

				ul.breadcrumbs-custom__path
					li: a(href='index.html') Home
					li: a(href='#') Elements
					li.active Footers

	section.section.section-xs.bg-gray-1.novi-background.bg-cover
		.shell.shell-wide: .range.range-md-center.range-fix: .cell-lg-11
			h3 Footer minimal

	footer.footer-corporate__aside.bg-gray-darker.text-center.novi-background.bg-cover
		.shell.shell-fluid.shell-condensed
			.range.range-20.range-fix.range_xl-ten.footer-corporate__range
				.cell-sm-8.cell-xl-6.footer-corporate__aside-column.text-sm-left
					include ../includes/_rights

				.cell-sm-4.cell-xl-4.footer-corporate__aside-column.text-sm-right
					ul.list-inline-xxs
						li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-twitter(href='#')
						li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-facebook(href='#')
						li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-instagram(href='#')

	section.section.section-xs.bg-gray-1.novi-background.bg-cover
		.shell.shell-wide: .range.range-md-center.range-fix: .cell-lg-11
			h3 Footer dark

	footer.footer-corporate
		.footer-corporate__main.bg-gray-darker.novi-background.bg-cover
			.shell.shell-fluid.shell-condensed
				.range.range-xs-center.range_xl-ten.range-50.footer-corporate__range
					.cell-xs-10.cell-sm-6.cell-md-4.cell-lg-4.cell-xl-2.footer-corporate__column
						h4.heading-bordered Main Office
						ul.list-md
							li
								p.address 6036 Richmond hwy., Alexandria, VA, 2230
								ul.list-inline-0
									li: dl.list-terms-inline
										dt Weekdays
										dd 8:00–20:00

									li: dl.list-terms-inline
										dt Weekends
										dd Closed
							li
								dl.list-terms-inline
									dt Sales department
									dd: a(href='mailto:#') info@demolink.org

								dl.list-terms-inline
									dt Support
									dd: a(href='mailto:#') infosupport@demolink.org

							li
								dl.list-terms-inline
									dt Phone
									dd: a(href='tel:#') +1 (409) 987–5874

					.cell-xs-10.cell-sm-6.cell-md-3.cell-lg-3.cell-xl-2.footer-corporate__column
						h4.heading-bordered Subscribe Now
						p Get the latest updates and offers
						// RD Mailform
						form.rd-mailform.rd-mailform_sm.rd-mailform_centered.box-width-2(data-form-output="form-output-global", data-form-type='subscribe', method='post', action='bat/rd-mailform.php')
							.form-wrap
								input#subscribe-email-1.form-input(type="email" name="email" data-constraints="@Email @Required")
								label.form-label(for="subscribe-email-1") Enter your e-mail
							button.button.button-sm.button-block.button-secondary(type="submit") Send

					.cell-xs-6.cell-sm-6.cell-md-3.cell-lg-3.footer-corporate__column
						h4.heading-bordered Latest News

						ul.post-group.post-light-group
							li
								article.post-light
									p.post-light__title: a(href=postLink) Email Newsletters Hit a Speed Bump with Inaccurate Open Rates
									time(datetime="2018") April 13, 2018
							li
								article.post-light
									p.post-light__title: a(href=postLink) How We Measure the Increasing Vlogs' Ad Potential
									time(datetime="2018") March 25, 2018

					.cell-xs-4.cell-sm-6.cell-md-2.cell-lg-2.cell-xl-1.footer-corporate__column
						h4.heading-bordered Links
						ul.list-xxs.list_darker
							li: a(href='index.html') Home
							li: +equalLink('About us')
							li: +equalLink('Services')
							li: +equalLink('Our Work')
							li: +equalLink('News')
							li: +equalLink('Contacts')

		.footer-corporate__aside.bg-gray-base.text-center.novi-background.bg-cover
			.shell.shell-fluid.shell-condensed
				.range.range-20.range-fix.range_xl-ten.footer-corporate__range
					.cell-sm-8.cell-xl-6.footer-corporate__aside-column.text-sm-left
						include ../includes/_rights

					.cell-sm-4.cell-xl-4.footer-corporate__aside-column.text-sm-right
						ul.list-inline-xxs
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-twitter(href='#')
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-facebook(href='#')
							li: a.icon.novi-icon.icon-xs.icon-style-modern.fa.fa-instagram(href='#')
	="\n"

	section.section.section-xs.bg-gray-1.novi-background.bg-cover
		.shell.shell-wide: .range.range-md-center.range-fix: .cell-lg-11
			h3 Footer light

	footer.footer-corporate.footer-corporate-light
